{
  "name": "react-cms",
  "version": "1.0.0",
  "description": "React CMS",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "keywords": [
    "extension",
    "react"
  ],
  "homepage": "https://huytrongnguyen.github.io/react-cms",
  "repository": {
    "type": "git",
    "url": "https://github.com/huytrongnguyen/react-cms.git"
  },
  "babel": {
    "presets": [ "env", "react" ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      ["root-import", { "rootPathSuffix": "src/js" }]
    ],
    "ignore": "node_modules"
  },
  "scripts": {
    "build": "gulp",
    "server": "babel-node server.js --presets env"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "d3":"4.9.1",
    "ext-react": "1.6.2",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-root-import": "5.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env":"1.5.2",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "del": "3.0.0",
    "express": "4.15.3",
    "gulp": "3.9.1",
    "gulp-sass": "3.1.0",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "3.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify":"3.9.0"
  }
}
