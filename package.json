{
  "name": "react-cms",
  "version": "1.0.0",
  "description": "React CMS",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "keywords": [
    "extension",
    "react"
  ],
  "homepage": "https://huytrongnguyen.github.io/react-cms",
  "repository": {
    "type": "git",
    "url": "https://github.com/huytrongnguyen/react-cms.git"
  },
  "babel": {
    "presets": [ "env", "react" ],
    "plugins": [
      ["root-import", { "rootPathSuffix": "src" }]
    ],
    "ignore": "node_modules"
  },
  "scripts": {
    "vendor": "gulp vendor",
    "build": "webpack -p --progress --colors --watch",
    "start": "babel-node server.js --presets env"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.0.0",
    "jquery": "3.3.1",
    "popper.js": "1.12.9",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-root-import": "5.1.0",
    "babel-preset-env":"1.6.1",
    "babel-preset-react": "6.24.1",
    "express": "4.16.2",
    "gulp": "3.9.1",
    "node-sass": "4.7.2",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.2",
    "webpack": "3.11.0"
  }
}
